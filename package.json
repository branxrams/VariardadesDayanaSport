    "debug": "~2.6.9",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^6.15.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^3.2.4",
    "sequelize": "^6.31.1"
  },
  "devDependencies": {
    "dotenv": "^16.3.1",
    "electron": "^23.3.5",
    "electron-builder": "^24.4.0",
    "nodemon": "^2.0.22"
  },
  "build": {
    "appId": "com.example.dayanasport",
    "productName": "Dayana Sport",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
